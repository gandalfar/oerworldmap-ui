{
  "name": "oerworldmap-ui",
  "version": "0.0.1",
  "description": "The user interface for https://oerworldmap.org/",
  "scripts": {
    "clean": "rimraf dist/",
    "server:dev": "NODE_ENV=development node src/index.js",
    "server:prod": "NODE_ENV=production node src/index.js",
    "build:prod": "NODE_ENV=production webpack-cli",
    "test": "eslint src --quiet && mocha --require babel-core/register --require ignore-styles test/helpers/browser.js test/*.js",
    "testrun": "mocha --require babel-core/register --inspect --require ignore-styles test/helpers/browser.js",
    "fix": "eslint src --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint src --ext .jsx,.js --quiet",
    "lint:style": "stylelint 'src/styles/**/*.pcss'",
    "site:dev": "NODE_ENV=development webpack-dev-server --config site.config.babel.js",
    "site:prod": "NODE_ENV=production webpack-cli --config site.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbz/oerworldmap-ui.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hbz/oerworldmap-ui/issues"
  },
  "homepage": "https://github.com/hbz/oerworldmap-ui#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.4",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-stylelint": "^8.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "jsdom": "^12.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/center": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@turf/invariant": "^6.1.2",
    "@turf/turf": "^5.1.6",
    "ajv": "^6.5.3",
    "asset-require-hook": "^1.2.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.3",
    "d3": "^5.7.0",
    "deepmerge": "^2.0.1",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.5",
    "express": "^4.16.3",
    "express-useragent": "^1.0.12",
    "font-awesome": "^4.7.0",
