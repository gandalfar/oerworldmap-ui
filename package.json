    "lodash": "^4.17.11",
    "mapbox-gl": "^0.49.0",
    "markdown-to-jsx": "^6.7.2",
    "mitt": "^1.1.3",
    "moment": "^2.22.1",
    "node-polyglot": "^2.3.0",
    "normalize.css": "^8.0.0",
    "path-to-regexp": "^2.4.0",
    "postcss-import": "^12.0.0",
    "postcss-preset-env": "^5.3.0",
    "prop-types": "^15.6.2",
    "rc-tooltip": "^3.7.3",
