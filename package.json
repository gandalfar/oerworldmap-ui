{
  "name": "oerworldmap-ui",
  "version": "0.0.1",
  "description": "The user interface for https://oerworldmap.org/",
  "scripts": {
    "clean": "rimraf dist/",
    "server:dev": "NODE_ENV=development node src/index.js",
    "server:static": "NODE_ENV=static node src/index.js",
    "server:prod": "NODE_ENV=production node src/index.js",
    "build:prod": "NODE_ENV=production webpack-cli",
    "test": "eslint src --quiet && mocha --require babel-core/register --require ignore-styles test/helpers/browser.js test/*.js",
    "testrun": "mocha --require babel-core/register --inspect --require ignore-styles test/helpers/browser.js",
    "fix": "eslint src --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint src --ext .jsx,.js --quiet",
    "lint:style": "stylelint 'src/styles/**/*.pcss'",
    "site:dev": "NODE_ENV=development webpack-dev-server --config site.config.babel.js",
    "site:prod": "NODE_ENV=production webpack-cli --config site.config.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hbz/oerworldmap-ui.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hbz/oerworldmap-ui/issues"
  },
  "homepage": "https://github.com/hbz/oerworldmap-ui#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-stylelint": "^8.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "jsdom": "^11.8.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "ajv": "^6.5.1",
    "asset-require-hook": "^1.2.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.2",
    "d3": "^5.5.0",
    "deepmerge": "^2.0.1",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "formdata-polyfill": "^3.0.11",
    "getenv": "^0.7.0",
    "http-link-header": "^0.8.0",
    "ignore-styles": "^5.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-pointer": "^0.6.0",
    "lodash": "^4.17.5",
    "mapbox-gl": "^0.46.0",
    "mitt": "^1.1.3",
    "moment": "^2.22.1",
    "node-polyglot": "^2.2.2",
    "normalize.css": "^8.0.0",
    "path-to-regexp": "^2.2.0",
    "postcss-import": "^11.1.0",
    "postcss-preset-env": "^5.2.1",
    "prop-types": "^15.6.2",
    "rc-tooltip": "^3.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-faux-dom": "^4.2.0",
    "react-markdown": "^3.3.4",
    "react-select": "^1.2.1",
    "react-simplemde-editor": "^3.6.16",
    "remove-markdown": "^0.3.0",
    "simplemde": "^1.11.2",
    "source-sans-pro": "git://github.com/adobe-fonts/source-sans-pro.git#release",
    "url-template": "^2.0.8",
    "webpack": "^4.12.2",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.1"
  }
}
